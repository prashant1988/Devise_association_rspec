      <td><%=link_to 'Edit', edit_emp_path(emp) %></td>
      <td><%=link_to 'Destroy',emp, :confirm=>'Are you sure?',method: :delete %></td>
      <td><%=link_to 'Asset_Check', :controller=>'assest',:action=>'index',:id=>emp.id %></td>

      <td><%= link_to 'Emp_check',emp_check_path(:id => emp.id) %></td>

  
  
  <%end %>
 </table>

