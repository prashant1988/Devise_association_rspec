  <% @emp.each do |emp| %>
    <tr>
      <td><%= emp.id %></td>
      <td><%= emp.name %></td>
      <td><%= emp.age %></td>
      <td><%= emp.address %></td>
      <td><%= emp.assests.size %></td>
